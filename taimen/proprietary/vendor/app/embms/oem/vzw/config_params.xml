<ReceptionGap>1</ReceptionGap>
<DelayJitter>PT2.5S</DelayJitter>
<r24:StreamingExtension>
<SegmentPeriodicityForDriftMonitoring xmlns="">1</SegmentPeriodicityForDriftMonitoring>
